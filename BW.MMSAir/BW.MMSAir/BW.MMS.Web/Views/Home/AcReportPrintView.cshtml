@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AcReportList</title>
    <script src="@Url.Content("~/Scripts/jquery-1.9.1.min.js")" type="text/javascript"></script>
    <style type="text/css">
        body
        {
            margin: 0px;
            padding: 0px;
        }
    </style>
    <script type="text/javascript">
        $(function () {
            $(document.body).height(document.body.parentElement.clientHeight);
            $(window).resize(function () {
                $(document.body).height(document.body.parentElement.clientHeight);
            });
            var url = decodeURIComponent('@ViewData["url"]');
            $.post(url).success(function (data) {
                if (data.success) {
                    var objAcDsg = document.getElementById('AcDesigner');
                    objAcDsg.OpenRepFromURL(data.message, 0);
                }
                else {
                    alert(data.message);
                }
            });
        });
    </script>
</head>
<body>
    <object id="AcDesigner" classid="clsid:62E6B2C0-43AF-4FEA-A0E5-0B496DA5A681" style="width: 100%;
        height: 100%;">
    </object>
</body>
</html>
